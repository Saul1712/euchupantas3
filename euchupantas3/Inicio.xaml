<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="euchupantas3.Inicio">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Inicio !" FontSize="24" HorizontalOptions="Center" VerticalOptions="CenterAndExpand" />
            <Image Source="logo1.png" HeightRequest="300" WidthRequest="300" />
            <Entry x:Name="txtDato" Placeholder="Ingrese dato uno"/>
            <Entry x:Name="txtDatoDos" Placeholder="Ingrese dato dos"/>
            <Button x:Name="btnAbrir" Text="Abri ventana dos" Clicked="btnAbrir_Clicked"/>
            <ImageButton Source="logo1.png"/>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>  